{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "appConfigName": {
            "value": "appconfig-wgrest-westus-pme"
        },
        "location": {
            "value": "westus"
        },
        "featureFlags": {
            "value": [
                {
                    "id": "GenevaLogging",
                    "description": "Feature flag to use Geneva Monitoring.",
                    "enabled": "true"
                },
                {
                    "id": "AuthorizationCertificate",
                    "description": "Feature flag to force certificate authorization.",
                    "enabled": "false"
                }
            ]
        }
    }
}